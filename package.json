{
  "name": "unlike.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "icons": "rm -rf ./components/icons/**/*.{tsx,ts} && npx @svgr/cli --typescript --icon --jsx-runtime automatic --no-index --out-dir components/icons -- _assets/icons",
    "svgs": "rm -rf ./components/svgs/**/*.{tsx,ts} && npx @svgr/cli --typescript  --icon '100%' --jsx-runtime automatic --no-index --out-dir components/svgs -- _assets/svgs"
  },
  "dependencies": {
    "@next/font": "13.0.4",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.4",
    "next": "13.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@svgr/cli": "6.5.1",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "autoprefixer": "10.4.13",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-tailwindcss": "3.7.0",
    "eslint-plugin-unicorn": "44.0.2",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "postcss": "8.4.19",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier -w -u"
    ],
    "!*.{js,ts,jsx,tsx}": "prettier -w -u"
  }
}
